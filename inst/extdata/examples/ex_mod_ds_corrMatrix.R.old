library(DaparViz)

data(vData_ft)
obj <- vData_ft[[1]]


ds_corrmatrix <- function(obj){
  ui <- fluidPage(
    mod_ds_corrmatrix_ui("plot")
  )
  
  server <- function(input, output, session) {
    mod_ds_corrmatrix_server("plot", reactive({obj}))
  }
  
  if (interactive())
    shinyApp(ui = ui, server = server)
}


